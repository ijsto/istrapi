{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "strapi-hook-bookshelf",
      "settings": {
        "client": "${process.env.IJS_ISTRAPI_DB_CLIENT}",
        "database": "${process.env.IJS_ISTRAPI_DB_DATABASE}",
        "host": "${process.env.IJS_ISTRAPI_DB_HOST}",
        "port": "${process.env.IJS_ISTRAPI_DB_PORT}",
        "username": "${process.env.IJS_ISTRAPI_DB_USERNAME}",
        "password": "${process.env.IJS_ISTRAPI_DB_PASSWORD}",
        "ssl": true
      },
      "options": {}
    }
  }
}
